<!DOCTYPE html>
<HTML>
<head>
    <title>Resume - View All</title>
</head>
<body>
<h1>Resumes</h1>

<a href="/resume/add/selectuser">Add Resume</a>

<br />
<br />

<table border=1">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Resume Name</th>
        <th>Actions</th>
    </tr>
    <% for(var i = 0; i < resumes.length; i++) { %>
    <tr>
        <td><%= resumes[i].first_name %></td>
        <td><%= resumes[i].last_name %></td>
        <td><%= resumes[i].resume_name %></td>
        <td>
            <a href="/resume/edit/?resume_id=<%= resumes[i].resume_id %>">
                Edit
            </a>
        </td>

    </tr>
    <% } %>
</table>

<br />
</body>
</HTML>